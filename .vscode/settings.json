{
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit"
  },
  "i18n-ally.localesPaths": [
    "src/translations"
  ],
  "deno.enable": false,
  "i18n-ally.keystyle": "flat",
  "eslint.lintTask.enable": false,
  "i18n-ally.refactor.templates": [
    {
      "default": true,
      "template": "{intl.formatMessage({ id: '{key}' })}",
    },
    {
      "default": false,
      "template": "intl.formatMessage({ id: '{key}' })",
    },
  ],
  "i18n-ally.sourceLanguage": "vi",
  "i18n-ally.displayLanguage": "en",
  "github.copilot.chat.codeGeneration.useInstructionFiles": true,
  "github.copilot.chat.codeGeneration.instructions": [
    {
      "text": "Use useAppSelector alternative to useSelector",
    },
    {
      "text": "Use useAppDispatch alternative to useDispatch",
    },
    {
      "text": "Use const intl = useIntl() to get intl object",
    }
  ]
}